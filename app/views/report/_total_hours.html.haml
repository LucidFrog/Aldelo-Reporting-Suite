-if @total_hours_data == []
  %h2
    No Data on Selected Date
-else
  %a{:href => "/report/export_total_hours?total_hours_date="+@date+"&job_title_id="+@job_title_id}
    Export to CSV File
  %br
  %br
  =link_to "Home", "/"
  %table
    %thead
      %tr
        %td FirstName
        %td LastName
        %td JobTitleText
        %td WorkDate
        %td OTHours
        %td RegularHours
        %td TotalHours
    %tbody
    -for employee in @total_hours_data
      %tr
        %td=employee[0]
        %td=employee[1]
        %td=employee[2]
        %td=Time.at(employee[3].to_i).strftime("%m/%d/%Y")
        %td=sprintf( "%.02f" , employee[4].to_f/60 )
        %td=sprintf( "%.02f" , employee[5].to_f/60 )
        %td=sprintf( "%.02f" , employee[6].to_f/60 )